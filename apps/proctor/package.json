{
  "name": "@ctrlplane/proctor",
  "version": "0.0.1",
  "description": "A process that listens to the kubernetes informer and sends to temporal",
  "engines": {
    "node": ">= 16.13 <17"
  },
  "type": "module",
  "scripts": {
    "predev": "node ./build.dev.js",
    "dev": "nodemon --enable-source-maps ./build/main.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@kubernetes/client-node": "^0.16.3",
    "rxjs": "^7.5.5",
    "temporalio": "^0.22.0",
    "ts-node": "^10.8.1",
    "typescript": "~4.6"
  },
  "author": "Yousuf Jawwad",
  "license": "Unlicensed",
  "devDependencies": {
    "@types/node": "^17.0.42",
    "esbuild": "^0.14.43",
    "nodemon": "^2.0.16",
    "tslib": "^2.4.0",
    "triple-beam": "^1.3.0",
    "util": "^0.12.4",
    "winston": "^3.7.2"
  },
  "nodemonConfig": {
    "restartable": "rs",
    "verbose": true,
    "watch": [
      "build/**/*.js"
    ],
    "ignore": [
      "*.js.map,",
      "*.d.ts"
    ],
    "ext": "js"
  }
}
