    "tslib": "~2.3.1",
    "typescript": "~4.6"
  },
  "volta": {
    "node": "16.13.0"
  },
  "dependencies": {
    "@ctrlplane.dev/env-ctrl-workflow": "*",
    "temporalio": "^0.22.0"
  }
